# Example environment configuration for QRLSAI
# Copy this file to .env and replace the placeholder values with your own secrets.

# Secret key used to sign JSON Web Token access tokens
JWT_ACCESS_SECRET=replace-with-access-secret

# Secret key used to sign JSON Web Token refresh tokens
JWT_REFRESH_SECRET=replace-with-refresh-secret

# Connection string for the Redis instance used by the application
REDIS_URL=redis://localhost:6379/0

# API key for accessing the OpenAI API
OPENAI_API_KEY=replace-with-openai-api-key

# API key for accessing the Anthropic API
ANTHROPIC_API_KEY=replace-with-anthropic-api-key

# API key for Google services integrations
GOOGLE_API_KEY=replace-with-google-api-key

# Default privacy lock setting (e.g., true or false)
PRIVACY_LOCK_DEFAULT=false
