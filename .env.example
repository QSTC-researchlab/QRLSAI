# Authentication secrets for signing JSON Web Tokens.
JWT_ACCESS_SECRET=your-access-token-secret
# Secret used for refreshing sessions; keep separate from the access secret.
JWT_REFRESH_SECRET=your-refresh-token-secret

# Connection string for the Redis instance used for caching and queues.
REDIS_URL=redis://localhost:6379/0

# API keys for third-party AI providers.
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
GOOGLE_API_KEY=your-google-api-key

# Default privacy lock setting applied to newly created resources (true/false).
PRIVACY_LOCK_DEFAULT=false
